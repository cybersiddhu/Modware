      <dbxref>
        <acc>cjm</acc>
        <dbname>GOC</dbname>
      </dbxref>
    </def>
    <is_obsolete>1</is_obsolete>
    <comment>this relation was superfluous, so it has been obsoleted</comment>
    <namespace>relationship</namespace>
  </typedef>
