    <id>OBO_REL:has_part</id>
    <alt_id>OBO_REL:0000003</alt_id>
    <name>has_part</name>
    <inverse_of_on_instance_level>OBO_REL:part_of</inverse_of_on_instance_level>
    <is_reflexive>1</is_reflexive>
