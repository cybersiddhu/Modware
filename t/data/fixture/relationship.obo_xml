  <typedef>
    <id>OBO_REL:integral_part_of</id>
    <alt_id>OBO_REL:0000004</alt_id>
    <name>integral_part_of</name>
    <is_a>OBO_REL:part_of</is_a>
    <def>
      <defstr>C integral_part_of C' if and only if: C part_of C' AND C' has_part C</defstr>
      <dbxref>
        <acc>15892874</acc>
        <dbname>PMID</dbname>
      </dbxref>
    </def>
    <inverse_of>OBO_REL:has_integral_part</inverse_of>
    <is_reflexive>1</is_reflexive>
    <is_anti_symmetric>1</is_anti_symmetric>
    <is_transitive>1</is_transitive>
    <namespace>relationship</namespace>
  </typedef>
